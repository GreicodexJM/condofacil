{
  "name": "vite2-vue3-pwa",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --host",
    "build-staging": "vite build --mode staging",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint src/**/*.{ts,vue} --fix",
    "format": "prettier src/**/*.{ts,vue} --write",
    "commit": "git-cz",
    "postinstall": "husky install"
